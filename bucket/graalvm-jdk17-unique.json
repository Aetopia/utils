    "version": "21.3.0",
    "homepage": "https://www.graalvm.org/",
    "license": "GPL-2.0",
    "url": "https://github.com/graalvm/graalvm-ce-builds/releases/download/vm-21.3.0/graalvm-ce-java17-windows-amd64-21.3.0.zip#dl.7z",
    "hash": "dd8b89ad60bf909327fdb6e5711d944c5e581d47add6414757ac9b293febc7be",
    "extract_dir": "graalvm-ce-java17-21.3.0",
    "bin": [
        [

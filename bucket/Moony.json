  "post_install": [
    "$lnk = Join-Path (Get-Command scoop.ps1 | Split-Path | Split-Path) shims\\mn.lnk",
    "$WshShell = New-Object -comObject WScript.Shell",
    "$Shortcut = $WshShell.CreateShortcut(\"$lnk\")",
